<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">

  <CommandSet xml:lang="en-gb" Name="examplevcd">
    <CommandPrefix>Ricoh</CommandPrefix>
    <Example>Command the printer!</Example>

    <Command Name="command">
      <Example>Print Hello World!</Example>
      <ListenFor>{command}</ListenFor>
      <Feedback>Commanding Ricoh to {command}...</Feedback>
      <Navigate/>
    </Command>
    
    <PhraseTopic Label="command">
      <Subject>print</Subject>
    </PhraseTopic>    

    <!--<PhraseList Label="command">
      <Item>print</Item>
      <Item>scan</Item>
      <Item>copy</Item>
    </PhraseList>-->
    
  </CommandSet>

</VoiceCommands>
